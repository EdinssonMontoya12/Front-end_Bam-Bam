<body>


  <body>

    <body>
      <div class="loader"></div>
      {{> sidebar-navbar }}
      <div class="main-wrapper main-wrapper-1">
        <div class="navbar-bg"></div>
        <!-- Main Content -->
        <div class="main-content">
          <section class="section">
            <div class="section-body">
              <!-- add content here -->
              <div class="card">
                <div class="card-header">
                  <h4>Ingresar Producto</h4>
                </div>
                <div class="form">
                  <div class="card-body">
                    <div class="row">
                      <div class="form-group col-sm-6">
                        <label>Codigo</label>
                        <input type="text" class="form-control">
                      </div>
                      <div class="form-group col-sm-6">
                        <label>Fecha Vencimiento </label>
                        <input type="date" class="form-control">
                      </div>
                    </div>
                    <div class="row">
                      <div class="form-group col-sm-6">
                        <label>Producto</label>
                        <select class="form-control">
                          <option selected>Options</option>
                          <option value="1">One</option>
                          <option value="2">Two</option>
                          <option value="3">Three</option>
                        </select>
                      </div>
                      <div class="form-group col-sm-6">
                        <label>Cantidad</label>
                        <input type="number" class="form-control">
                      </div>
                    </div>
                  </div>
                  <div class="card-footer">
                    <div class="row">
                      <div class="col clearfix">
                        <button type="submit" class="btn btn-primary col-sm-3 float-right">Crear</button>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
        </div>
        </section>
      </div>
      <footer class="main-footer">
        <div class="footer-left">
          Copyright &copy; 2019 <div class="bullet"></div> Design By <a href="#">Redstar</a>
        </div>
        <div class="footer-right">
        </div>
      </footer>

      <script type="text/javascript">
        function EnableDisableTB() {
          var si = document.getElementById("si");
          var otherlan = document.getElementById("otherlan");
          var otherlan1 = document.getElementById("otherlan1");
          var otherlan2 = document.getElementById("otherlan2");
          otherlan.disabled = si.checked ? false : true;
          otherlan1.disabled = si.checked ? false : true;
          otherlan2.disabled = si.checked ? false : true;
          otherlan1.value = "";
          if (!otherlan1.disabled) {
            otherlan1.focus();
          }
          otherlan2.value = "";
          if (!otherlan2.disabled) {
            otherlan2.focus();
          }
          otherlan.value = "";
          if (!otherlan.disabled) {
            otherlan.focus();
          }
        }
      </script>
      </div>
      </section>

      <!-- General JS Scripts -->
      <script src="/assets/js/app.min.js"></script>
      <!-- JS Libraies -->
      <!-- Page Specific JS File -->
      <!-- Template JS File -->
      <script src="/assets/js/scripts.js"></script>
      <!-- Custom JS File -->
      <script src="/assets/js/custom.js"></script>
    </body>

</html>